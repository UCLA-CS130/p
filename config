port 8080;

path / ProxyHandler {
	host www.ucla.edu;	
}

path /echo EchoHandler {
	root /echo;
}

path /static StaticHandler {
	root  file/path0;
}

path /static1 StaticHandler {
	root  file/path1;
}

path /static2 StaticHandler {
	root  file/path2;
}

path /markdown StaticHandler {
	root  file/markdown;
}

path /status StatusHandler {
	root /status;
}

default NotFoundHandler {
	root;
}
